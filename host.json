{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "logging": {
    "logLevel": {
      "Host.Results": "Information",
      "Function": "Information",
      "default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false
      }
    }
  },
  "functionTimeout": "00:05:00",
  "extensions": {
    "queues": {
      "maxDequeueCount": 5
    },
    "blobs": {
      "maxDegreeOfParallelism": 1
    }
  }
}




